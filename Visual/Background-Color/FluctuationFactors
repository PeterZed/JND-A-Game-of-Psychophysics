function FluctuationFactors() {
  this.time = {
    waitTimeMean: 1000, // milliseconds
    waitTimeStandardDeviation: 0 // milliseconds;
  };
  
  this.hue = {
    fluctuationMean: 0,
    fluctuationStdDev: 1
  };
  
  this.saturation = {
    fluctuationMean: 0,
    fluctuationStdDev: 1
  };

  this.lightness = {
    fluctuationMean: 0,
    fluctuationStdDev: 1
  };
}
