function determineChangingAttribute() {
 var randomRoll = getRandom();
 
 if (randomRoll <= 1/3) {
  return fluctuationDeterminantList.hue;
 } else if (randomRoll <= 2/3) {
  return fluctuationDeterminantList.saturation;
 } else if (randomRoll <= 1) {
  return fluctuationDeterminantList.lightness;
 }
}
